{
    "name": "Dashboard",
    "version": "1.0",
    "description": "This Chrome extension features a dashboard that appears in every new tab",
    "manifest_version": 2,
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "128": "icon.png"
    }
}